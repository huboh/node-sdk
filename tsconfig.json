{
 "compilerOptions": {
 "watch": true,
 "incremental": false, 
 "target": "ES2017",
 "module": "commonjs",
 "outDir": "./dist", 
 "rootDir": "./lib", 
 "composite": false,
 "removeComments": true,
 "strict": true,
 "noUnusedLocals": true, 
 "noUnusedParameters": true,  
 "moduleResolution": "node",
 "esModuleInterop": true, 
 "skipLibCheck": true, 
 "forceConsistentCasingInFileNames": true 
 }
}
